require python [ blacklist="2.6 3.1 3.2 3.3" min_versions="2.7.5" python_opts="" ]

SUMMARY="Convert GIF images (or even whole web hierarchies) to PNG format"
HOMEPAGE="http://www.catb.org/~esr/gif2png"
DOWNLOADS="${HOMEPAGE}/${PNV}.tar.gz"

LICENCES="libpng ZLIB"
SLOT="0"
PLATFORMS="~amd64 ~x86"
MYOPTIONS=""

DEPENDENCIES="
    build+run:
        media-libs/libpng:=
"

